{
  "imports": {
    "preact": "npm:preact@10.19.6",
    "preact/hooks": "npm:preact@10.19.6/hooks",
    "preact/jsx-runtime": "npm:preact@10.19.6/jsx-runtime",
    "preact-render-to-string": "npm:preact-render-to-string@6.3.1",
    "@hotwired/turbo": "npm:@hotwired/turbo@8.0.0",
    "@/": "./app/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "tasks": {
    "build": "deno run --allow-all build.ts",
    "watch": "deno run --allow-all build.ts --watch"
  }
}
