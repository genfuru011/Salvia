    "preact/jsx-runtime": "https://esm.sh/preact@10.19.2/jsx-runtime",
    "preact-render-to-string": "https://esm.sh/preact-render-to-string@6.3.1?external=preact",
    "@hotwired/turbo": "https://esm.sh/@hotwired/turbo@8.0.0",
