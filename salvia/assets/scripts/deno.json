{
  "tasks": {
    "build": "deno run --allow-all salvia/build.ts",
    "watch": "deno run --allow-all --watch salvia/build.ts --watch"
  },
  "imports": {
    "preact": "https://esm.sh/preact@10.19.2",
    "preact/hooks": "https://esm.sh/preact@10.19.2/hooks",
    "preact/jsx-runtime": "https://esm.sh/preact@10.19.2/jsx-runtime",
    "preact-render-to-string": "https://esm.sh/preact-render-to-string@6.3.1?external=preact",
    "@/": "../app/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
