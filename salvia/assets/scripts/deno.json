  "imports": {
    "framework": "https://esm.sh/preact@10.19.2",
    "framework/hooks": "https://esm.sh/preact@10.19.2/hooks",
    "framework/jsx-runtime": "https://esm.sh/preact@10.19.2/jsx-runtime",
    "framework/ssr": "https://esm.sh/preact-render-to-string@6.3.1?external=preact",
